h1{
	font-family: 'Open Sans';
	font-size: 30px;
	line-height: 22px;
	font-weight: 700;
	letter-spacing: 0.05em;
	color: #212121;
	text-align: center;
	padding-top: 10%;
}

h2{

	font-family: 'Open Sans';
	font-size: 20px;
	line-height: 22px;
	font-weight: 600;
	letter-spacing: 0.05em;
	color: #212121;	
}

p{

	font-family: 'Open Sans';
	font-size: 15px;
	line-height: 22px;
	font-weight: 400;
	letter-spacing: 0.05em;
	color: #212121;
	text-align: center;
	padding-top: 1%;
}

.main_container{
	width: 100%;
	background-color: #ffffff;
	margin: 0 auto;
	justify-content: center;
	text-align:center;

}

.buttons_container{
	display: flex;
	flex-basis: auto;
	flex-wrap:wrap;
	
	
	
}

.button_green{
	background-color:#00897b;
	width: 100px;
	height: 55px;
	
	margin-left: 50px;
	margin-right: 50px;
	margin-top: 4%;
	
}

.button{
	background-color:#ffffff;
	width: 179px;
	height: 51px;
	border: 2px solid #00897b;
	
	margin-left: 50px;
	margin-right: 50px;
	margin-top: 4%;
}

.images_container{
	display: flex;
	flex-basis: auto;
	flex-wrap:wrap;
	justify-content: center;
}

.images{
	margin-left: 25px;
	margin-right: 25px;
	margin-top: 25px;
}